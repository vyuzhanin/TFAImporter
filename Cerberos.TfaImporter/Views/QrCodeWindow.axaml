<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="using:Cerberos.TfaImporter.ViewModels"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="400"
        x:Class="Cerberos.TfaImporter.Views.QrCodeWindow"  SizeToContent="WidthAndHeight"
        Title="Qr Code">
    <Design.DataContext>
        <vm:QrCodeWindowViewModel />
    </Design.DataContext>
    <Image Width="400" Height="400" Source="{Binding GeneratedQrCode}"></Image>
</Window>
